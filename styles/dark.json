{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Dark",
	"settings": {
		"color": {
			"palette": [
				{
					"color": "#000000",
					"name": "Black",
					"slug": "black"
				},
				{
					"color": "#ffffff",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "#f5f5f5",
					"name": "Light Gray",
					"slug": "light-gray"
				},
				{
					"color": "#cccccc",
					"name": "Medium Gray",
					"slug": "medium-gray"
				},
				{
					"color": "#666666",
					"name": "Dark Gray",
					"slug": "dark-gray"
				},
				{
					"color": "#1a1a1a",
					"name": "Off Black",
					"slug": "off-black"
				},
				{
					"color": "#40a8d8",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#5bc0de",
					"name": "Primary Light",
					"slug": "primary-light"
				},
				{
					"color": "#2e7da2",
					"name": "Primary Dark",
					"slug": "primary-dark"
				}
			]
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--black)",
			"text": "var(--wp--preset--color--white)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary-light)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					}
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--white)"
				}
			}
		},
		"blocks": {
			"core/site-title": {
				"color": {
					"text": "var(--wp--preset--color--white)"
				}
			},
			"core/navigation": {
				"color": {
					"text": "var(--wp--preset--color--light-gray)"
				}
			},
			"core/post-title": {
				"color": {
					"text": "var(--wp--preset--color--white)"
				}
			},
			"core/post-content": {
				"color": {
					"text": "var(--wp--preset--color--light-gray)"
				}
			},
			"core/separator": {
				"color": {
					"text": "var(--wp--preset--color--dark-gray)"
				},
				"border": {
					"color": "var(--wp--preset--color--dark-gray)"
				}
			},
			"core/pullquote": {
				"border": {
					"color": "var(--wp--preset--color--primary-light)"
				}
			},
			"core/quote": {
				"border": {
					"color": "var(--wp--preset--color--primary-light)"
				}
			}
		}
	}
}