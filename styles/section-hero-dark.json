{
  "name": "hero-dark",
  "label": "Hero Band (Dark)",
  "description": "Dark hero band with fluid spacing and display typography",
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--neutral-800)",
      "text": "var(--wp--preset--color--base)"
    },
    "spacing": {
      "padding": {
        "top": "var(--wp--preset--spacing--90)",
        "bottom": "var(--wp--preset--spacing--90)",
        "left": "var(--wp--preset--spacing--50)",
        "right": "var(--wp--preset--spacing--50)"
      }
    },
    "typography": {
      "textAlign": "center"
    },
    "elements": {
      "heading": {
        "typography": {
          "fontWeight": "700",
          "fontSize": "var(--wp--preset--font-size--800)"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--accent)"
        },
        ":hover": {
          "color": {
            "text": "var(--wp--preset--color--primary)"
          }
        }
      }
    },
    "blocks": {
      "core/heading": {
        "typography": {
          "fontSize": "var(--wp--preset--font-size--800)"
        }
      }
    }
  }
}
