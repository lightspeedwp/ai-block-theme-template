{
  "$schema": "https://schemas.wp.org/wp/6.7/theme.json",
  "version": 3,
  "title": "Condensed Sans",
  "slug": "condensed-sans",
  "styles": {
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--condensed-sans)"
    },
    "elements": {
      "heading": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--condensed-sans)",
          "fontWeight": "800",
          "letterSpacing": "-0.5px"
        }
      }
    }
  },
  "settings": {
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "Oswald, Arial Narrow, sans-serif",
          "name": "Condensed Sans",
          "slug": "condensed-sans",
          "fontFace": [
            {
              "fontDisplay": "swap",
              "fontFamily": "Oswald",
              "fontStyle": "normal",
              "fontWeight": "400 900",
              "src": ["file:./assets/fonts/oswald/Oswald-VariableFont_wght.ttf"]
            }
          ]
        }
      ]
    }
  }
}